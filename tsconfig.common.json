{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es2019",
    "declaration": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "removeComments": false,
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
