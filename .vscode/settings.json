{
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/node_modules": true,
    "src/**/*.js": true,
    "**/dist": true,
    "**/.yalam-cache": true,
    "logs": true,
    "yarn.lock": true,
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true
}
